<h1>Log In</h1>
<%= form_for(:session, url: sessions_create_path) do |f| %>
    <%= f.label :email, "Email" %>
    <%= f.email_field :email, placeholder: "Email" %><br>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password, placeholder: "Password" %><br>
    <%= f.submit "Log In"%>
<% end %>
